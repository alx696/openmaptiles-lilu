{
  "version": 8,
  "name": "OSM OpenMapTiles",
  "id": "openmaptiles",
  "center": [
    0,
    0
  ],
  "zoom": 1,
  "bearing": 0,
  "pitch": 0,
  "sprite": "http://localhost/map/sprite/sprite",
  "glyphs": "http://localhost/map/glyphs/{fontstack}/{range}.pbf",
  "sources": {
    "openmaptiles": {
      "type": "vector",
      "url": "http://localhost:8090"
    },
    "arcgis_online": {
      "type": "raster",
      "minzoom": 4,
      "maxzoom": 20,
      "tileSize": 256,
      "tiles": [
        "https://server.arcgisonline.com/arcgis/rest/services/World_Imagery/MapServer/tile/{z}/{y}/{x}"
      ]
    }
  },
  "layers": [
    {
      "id": "background",
      "type": "background",
      "layout": {
        "visibility": "visible"
      },
      "paint": {
        "background-color": "#f2efe9"
      }
    },
    {
      "id": "arcgis_online",
      "type": "raster",
      "metadata": {
        "maputnik:comment": "ArcGIS卫星在线"
      },
      "source": "arcgis_online",
      "layout": {
        "visibility": "visible"
      }
    }
  ]
}
